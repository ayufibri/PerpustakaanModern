package si_perpustakaan.modern.dan.efisien;

import java.util.ArrayList;

public class TransaksiPengembalian {
  private String tanggal;
  private String waktu;
  private Buku buku;
  private String anggotaa;

  public TransaksiPengembalian(String tanggal, String waktu, Buku buku, String anggotaa) {
    this.tanggal = tanggal;
    this.waktu = waktu;
    this.buku = buku;
    this.anggotaa = anggotaa;
  }

  public void catatPengembalian() {
    System.out.println("Pengembalian dicatat pada " + tanggal + " " + waktu);
  }

  public Buku getBuku() {
    return buku;
  }

  public String getAnggotaa() {
    return anggotaa;
  }

  public void setTanggal(String tanggal) {
    this.tanggal = tanggal;
  }

  public void setWaktu(String waktu) {
    this.waktu = waktu;
  }

  public void setBuku(Buku buku) {
    this.buku = buku;
  }

  public void setAnggotaa(String anggotaa) {
    this.anggotaa = anggotaa;
  }
}
