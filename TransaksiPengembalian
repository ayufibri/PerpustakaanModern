package si_perpustakaan.modern.dan.efisien;

import java.util.ArrayList;

/**
 *
 * @author ASUS
 */
public class TransaksiPengembalian {
    private String tanggal;
    private String waktu;
    private Buku buku;
    private String anggota;

    public TransaksiPengembalian(String tanggal, String waktu, Buku buku, String anggota) {
        this.tanggal = tanggal;
        this.waktu = waktu;
        this.buku = buku;
        this.anggota = anggota;
    }
    
    public void catatPengembalian() {
        System.out.println("Pengembalian dicatat pada " + tanggal + " " + waktu);
    }

    public Buku getBuku() {
        return buku;
    }

    public String getAnggota() {
        return anggota;
    }

    public void setTanggal(String tanggal) {
        this.tanggal = tanggal;
    }

    public void setWaktu(String waktu) {
        this.waktu = waktu;
    }

    public void setBuku(Buku buku) {
        this.buku = buku;
    }

    public void setAnggota(String anggota) {
        this.anggota = anggota;
    }
}
