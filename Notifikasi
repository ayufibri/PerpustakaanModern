package si_perpustakaan.modern.dan.efisien;

public class Notifikasi {
     private String batas_waktu; 
    private String info_terbaru; 

    public void kirimNotifikasi() {
        // pengiriman notifikasi
        System.out.println("Notifikasi dikirim: " + info_terbaru);
    }

    public String getBatasWaktu() {
        return batas_waktu;
    }

    public void setBatasWaktu(String batas_waktu) {
        this.batas_waktu = batas_waktu;
    }

    public String getInfoTerbaru() {
        return info_terbaru;
    }

    public void setInfoTerbaru(String info_terbaru) {
        this.info_terbaru = info_terbaru;
     }
}
