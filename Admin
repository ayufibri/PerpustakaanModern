    // Metode-metode lain tetap sama

    public void tampilkanDaftarAnggota() {
        System.out.println("Daftar Anggota:");
        for (AnggotaPerpustakaan anggota : daftarAnggota) {
            System.out.println(anggota.getNama()); // Menggunakan metode getNama() atau metode lain yang sesuai
        }
    }

